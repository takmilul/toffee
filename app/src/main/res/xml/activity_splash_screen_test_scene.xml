        <OnClick motion:targetId="@+id/button" />
    </Transition>

    <ConstraintSet android:id="@+id/start"></ConstraintSet>

    <ConstraintSet
        android:id="@+id/end"
        motion:transitionEasing="decelerate">
        <Constraint
            android:id="@+id/splashLineImageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="4dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintDimensionRatio="5.8:5.6"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:transitionEasing="decelerate" >
            <Motion motion:transitionEasing="decelerate"/>
        </Constraint>
        <Constraint
            android:id="@+id/splashPinkImageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="32dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHeight_percent="0.50"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.52"
            motion:layout_constraintWidth_percent="0.99">
            <!--            <Transform android:rotation="0" />-->
        </Constraint>
        <Constraint
            android:id="@+id/splashWhiteImageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="6dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHeight_percent="0.44"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.52"
            motion:layout_constraintWidth_percent="0.70" />
        <Constraint
            android:id="@+id/splashLogoImageView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="22dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHeight_percent="0.09"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.58"
            motion:layout_constraintWidth_percent="0.26" >
            <Motion motion:transitionEasing="accelerate" />
            <PropertySet motion:motionProgress="1"/>
        </Constraint>
    </ConstraintSet>
</MotionScene>