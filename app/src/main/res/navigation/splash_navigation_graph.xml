<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/splash_navigation_graph"
    app:startDestination="@id/splashScreenFragment">


    <fragment
        android:id="@+id/splashScreenFragment"
        android:name="com.banglalink.toffee.ui.splash.SplashScreenFragment"
        android:label="SplashScreenFragment"
        tools:layout="@layout/fragment_splash_screen">

        <action
            android:id="@+id/action_splashScreenFragment_to_dynamicSplashScreenFragment"
            app:destination="@id/dynamicSplashScreenFragment" />
    </fragment>

    <fragment
        android:id="@+id/dynamicSplashScreenFragment"
        android:name="com.banglalink.toffee.ui.splash.DynamicSplashScreenFragment"
        android:label="DynamicSplashScreenFragment"
        tools:layout="@layout/fragment_dynamic_splash_screen">

    </fragment>
</navigation>