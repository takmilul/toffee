<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_navigation_graph"
    app:startDestination="@id/menu_feed">
    <fragment
        android:id="@+id/menu_activities"
        android:name="com.banglalink.toffee.ui.useractivities.UserActivitiesMainFragment"
        android:label="UserActivitiesMainFragment" >
        <action
            android:id="@+id/action_menu_activities_to_myChannelRatingFragment"
            app:destination="@id/myChannelRatingFragment" />
    </fragment>
    <fragment
        android:id="@+id/menu_feed"
        android:name="com.banglalink.toffee.ui.home.LandingPageFragment"
        android:label="FeedFragment">
        <action
            android:id="@+id/action_landingPageFragment_to_allCategoriesFragment"
            app:destination="@id/allCategoriesFragment" />
        <action
            android:id="@+id/action_landingPageFragment_to_categoryDetailsFragment"
            app:destination="@id/categoryDetailsFragment" />
        <action
            android:id="@+id/action_menu_feed_to_menu_tv"
            app:destination="@id/menu_tv" />
        <action
            android:id="@+id/action_menu_feed_to_menu_activities"
            app:destination="@id/menu_activities" />
        <action
            android:id="@+id/action_menu_feed_to_menu_channel"
            app:destination="@id/menu_channel" />
        <action
            android:id="@+id/action_menu_feed_to_myChannelRatingFragment"
            app:destination="@id/myChannelRatingFragment" />
        <action
            android:id="@+id/action_landingCategoriesFragment_to_categoryDetailsFragment"
            app:destination="@id/categoryDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/menu_tv"
        android:name="com.banglalink.toffee.ui.channels.ChannelFragment"
        android:label="ChannelFragment" >
        <argument
            android:name="title"
            app:argType="string"
            android:defaultValue="Tv Channels" />
        <argument
            android:name="category"
            app:argType="string" />
        <argument android:name="sub-category"
            app:argType="string" />
        <argument
            android:name="sub-category-id"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/menu_channel"
        android:name="com.banglalink.toffee.ui.mychannel.MyChannelHomeFragment"
        android:label="MyChannelRatingFragment">
        <action
            android:id="@+id/action_menu_channel_to_myChannelEditFragment"
            app:destination="@id/MyChannelEditFragment" />
        <action
            android:id="@+id/action_menu_channel_to_myChannelPlaylistVideosFragment"
            app:destination="@id/myChannelPlaylistVideosFragment" />
        <action
            android:id="@+id/action_menu_channel_to_challengesFragment"
            app:destination="@id/challengesFragment" />
        <!--<argument
            android:name="isOwner"
            app:argType="integer" />
        <argument
            android:name="isSubscribed"
            app:argType="integer" />
        <argument
            android:name="channelId"
            app:argType="integer" />-->
    </fragment>
    <fragment
        android:id="@+id/uploadMethodFragment"
        android:name="com.banglalink.toffee.ui.upload.UploadMethodFragment"
        android:label="UploadMethodFragment" >
        <action
            android:id="@+id/action_uploadMethodFragment_to_editUploadInfoFragment"
            app:destination="@id/editUploadInfoFragment"
            app:popUpTo="@id/uploadMethodFragment"
            app:popUpToInclusive="true"
            />
    </fragment>
    <fragment
        android:id="@+id/editUploadInfoFragment"
        android:name="com.banglalink.toffee.ui.upload.EditUploadInfoFragment"
        android:label="EditUploadInfoFragment" >
        <action
            android:id="@+id/action_editUploadInfoFragment_to_thumbnailSelectionMethodFragment"
            app:destination="@id/thumbnailSelectionMethodFragment" />
    </fragment>

    <fragment
        android:id="@+id/allCategoriesFragment"
        android:name="com.banglalink.toffee.ui.landing.AllCategoriesFragment"
        android:label="AllCategoriesFragment" >
        <action
            android:id="@+id/action_allCategoriesFragment_to_categoryDetailsFragment"
            app:destination="@id/categoryDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/categoryDetailsFragment"
        android:name="com.banglalink.toffee.ui.category.CategoryDetailsFragment"
        android:label="CategoryDetailsFragment" />
    <fragment
        android:id="@+id/MyChannelEditFragment"
        android:name="com.banglalink.toffee.ui.mychannel.MyChannelEditDetailFragment"
        android:label="MyChannelEditFragment">
        <argument
            android:name="myChannelDetail"
            app:nullable="true"
            app:argType="com.banglalink.toffee.model.MyChannelDetail" />
    </fragment>

<!--    <include-->
<!--        app:graph="@navigation/drawer_navigation_grapg" />-->
    <fragment
        android:id="@+id/myChannelRatingFragment"
        android:name="com.banglalink.toffee.ui.mychannel.MyChannelHomeFragment"
        android:label="MyChannelRatingFragment" />
    <fragment
        android:id="@+id/myChannelPlaylistVideosFragment"
        android:name="com.banglalink.toffee.ui.mychannel.MyChannelPlaylistVideosFragment"
        android:label="MyChannelPlaylistVideosFragment">
        <argument
            android:name="channelId"
            app:argType="integer" />
        <argument
            android:name="isOwner"
            app:argType="integer" />
        <argument
            android:name="playlistId"
            app:argType="integer" />
    </fragment>
    <fragment
        android:id="@+id/thumbnailSelectionMethodFragment"
        android:name="com.banglalink.toffee.ui.upload.ThumbnailSelectionMethodFragment"
        android:label="ThumbnailSelectionMethodFragment" />
    <fragment
        android:id="@+id/earningsFragment"
        android:name="com.banglalink.toffee.ui.earnings.EarningsFragment"
        android:label="EarningsFragment" >
        <action
            android:id="@+id/action_earningsFragment_to_addNewPaymentMethodFragment"
            app:destination="@id/addNewPaymentMethodFragment" />
        <action
            android:id="@+id/action_earningsFragment_to_reviewWithdrawalFragment"
            app:destination="@id/reviewWithdrawalFragment" />
    </fragment>
    <fragment
        android:id="@+id/addNewPaymentMethodFragment"
        android:name="com.banglalink.toffee.ui.earnings.AddNewPaymentMethodFragment"
        android:label="fragment_add_new_payment_method"
        tools:layout="@layout/fragment_add_new_payment_method" />
    <fragment
        android:id="@+id/reviewWithdrawalFragment"
        android:name="com.banglalink.toffee.ui.earnings.ReviewWithdrawalFragment"
        android:label="fragment_review_withdrawal"
        tools:layout="@layout/fragment_review_withdrawal" >
        <argument
            android:name="withdrawalAmount"
            app:argType="string" />
        <argument
            android:name="paymentMethod"
            app:argType="com.banglalink.toffee.model.PaymentMethod" />
        <action
            android:id="@+id/action_reviewWithdrawalFragment_to_withdrawalSuccessFragment"
            app:destination="@id/withdrawalSuccessFragment" />
    </fragment>
    <fragment
        android:id="@+id/withdrawalSuccessFragment"
        android:name="com.banglalink.toffee.ui.earnings.WithdrawalSuccessFragment"
        android:label="fragment_withdrawal_success"
        tools:layout="@layout/fragment_withdrawal_success" >
        <action
            android:id="@+id/action_withdrawalSuccessFragment_to_earningsFragment"
            app:destination="@id/earningsFragment"
            app:popUpTo="@id/earningsFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationDropdownFragment"
        android:name="com.banglalink.toffee.ui.notification.NotificationDropdownFragment"
        android:label="NotificationDropdownFragment" >
        <action
            android:id="@+id/action_notificationDropdownFragment_to_notificationDetailFragment"
            app:destination="@id/notificationDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/notificationDetailFragment"
        android:name="com.banglalink.toffee.ui.notification.NotificationDetailFragment"
        android:label="fragment_notification_detail"
        tools:layout="@layout/fragment_notification_detail" >
        <argument
            android:name="notificationInfo"
            app:argType="com.banglalink.toffee.data.database.entities.NotificationInfo" />
    </fragment>
    <fragment
        android:id="@+id/challengesFragment"
        android:name="com.banglalink.toffee.ui.challenge.ChallengeFragment"
        android:label="ChallengesFragment" >
        <action
            android:id="@+id/action_challengesFragment_to_challengeResultFragment"
            app:destination="@id/challengeResultFragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeResultFragment"
        android:name="com.banglalink.toffee.ui.challenge.ChallengeResultFragment"
        android:label="ChallengeResultFragment" >
        <action
            android:id="@+id/action_challengeResultFragment_to_challengeDetailFragment"
            app:destination="@id/challengeDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/challengeDetailFragment"
        android:name="com.banglalink.toffee.ui.challenge.ChallengeDetailFragment"
        android:label="ChallengeDetailFragment" />
</navigation>