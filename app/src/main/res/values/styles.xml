    <style name="SingleLineTextFieldStyle" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">@dimen/single_line_input_text_height</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:background">@drawable/single_line_input_text_bg</item>
        <item name="android:maxLines">1</item>
        <item name="android:inputType">textPersonName</item>
        <item name="android:ems">10</item>
        <item name="singleLine">true</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">@color/cardTitleColor</item>
        <item name="android:textSize">@dimen/input_field_text_size</item>
        <item name="android:textColorHint">@color/switchUnCheckedColor</item>
    </style>
    
    <style name="MultilineInputFieldStyle" parent="Widget.AppCompat.EditText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:layout_marginEnd">16dp</item>
        <item name="android:background">@drawable/multiline_input_text_bg</item>
        <item name="android:lines">4</item>
        <item name="android:maxLines">8</item>
        <item name="android:inputType">textMultiLine|textCapSentences</item>
        <item name="android:ems">10</item>
        <item name="android:maxLength">2000</item>
        <item name="android:gravity">top|start</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textColor">@color/cardTitleColor</item>
        <item name="android:textSize">@dimen/input_field_text_size</item>
        <item name="android:textColorHint">@color/switchUnCheckedColor</item>
    </style>
    
    <style name="LeadingIconSingleLineInputFieldStyle" parent="SingleLineInputFieldStyle">
        <item name="android:drawablePadding">12dp</item>
        <item name="android:inputType">textCapWords</item>
    </style>

    <style name="html_dialog" parent="Theme.AppCompat.Dialog.Alert">
        <item name="windowMinWidthMajor">90%</item>
        <item name="windowMinWidthMinor">90%</item>
    </style>

    <style name="ErrorTextStyle" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:layout_marginStart">12dp</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="android:maxLines">2</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textColor">@color/pink_to_accent_color</item>
        <item name="android:textSize">@dimen/default_text_size</item>
        <item name="android:visibility">gone</item>
    </style>
    
    <style name="fireworkVideoTitleStyle">
        <item name="android:lines">2</item>
        <item name="android:maxLines">2</item>
        <item name="android:gravity">bottom</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:fontFamily">@font/roboto_regular</item>
        <item name="android:textSize">@dimen/default_text_size</item>
        <item name="android:background">@drawable/overlay_bottom_round_bg</item>
    </style>
    
    <style name="fireworkImageStyle" >
        <item name="android:radius">12dp</item>
    </style>
    
    <style name="ButtonStyleNativeAd" parent="ToffeeNativeAdButtonStyle">
        <item name="backgroundTint">@color/native_ad_button_color</item>
        <item name="android:textColor">@color/white_transparency</item>
    </style>
    
    <style name="ToffeeNativeAdButtonStyle" parent="Widget.MaterialComponents.Button.TextButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/button_height</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:textSize">@dimen/button_text_size</item>
        <item name="android:fontFamily">@font/roboto_bold</item>
        <item name="android:letterSpacing">0</item>
        <item name="cornerRadius">5dp</item>
        <item name="iconGravity">textStart</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
    </style>
    
    <style name="AppTheme.AdAttribution">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:textSize">12sp</item>
        <item name="android:text">@string/ad_attribution</item>
        <item name="android:background">#FFCC66</item>
    </style>
    
    <style name="BubbleCountdownTitleStyle">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:gravity">center</item>
        <item name="android:text">0</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>
    
    <style name="BubbleCountdownSubtitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">4dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>
    
    <style name="BubbleSeparatorStyle">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginVertical">8dp</item>
        <item name="android:background">@color/bubble_separator_color</item>
    </style>
    
    <style name="BubbleFixtureTitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>
    
    <style name="BubbleFixtureSubtitleStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textSize">10sp</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="fontFamily">@font/roboto_regular</item>
    </style>
</resources>